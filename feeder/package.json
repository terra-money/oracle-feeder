    "promptly": "^3.2.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
